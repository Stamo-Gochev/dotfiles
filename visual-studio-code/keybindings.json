// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+k ctrl+d",
        "command": "editor.action.format",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+e",
        "command": "editor.action.insertCursorAtEndOfEachLineSelected",
        "when": "editorTextFocus"
    },
    {
        "key": "F8",
        "command": "workbench.action.compareEditor.nextChange",
        "when": "textCompareEditorVisible"
    },
    {
        "key": "F7",
        "command": "workbench.action.compareEditor.previousChange",
        "when": "textCompareEditorVisible"
    },
    {
        "key": "ctrl+,",
        "command": "workbench.action.openGlobalSettings"
    },
    {
        "key": "ctrl+m",
        "command": "workbench.action.gotoSymbol"
    },
    {
        "key": "ctrl+k ctrl+s",
        "command": "workbench.action.openGlobalKeybindings"
    },
    {
        "key": "ctrl+w",
        "command": "workbench.action.closeActiveEditor"
    },
    {
        "key": "f8",
        "command": "workbench.action.debug.stepOut",
        "when": "inDebugMode"
    },
    {
        "key": "ctrl+shift+i",
        "command": "workbench.debug.action.toggleRepl"
    },
    {
        "key": "F9",
        "command": "editor.debug.action.toggleBreakpoint",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+f",
        "command": "editor.action.extensioneditor.showfind",
        "when": "extensionEditorWebviewFocus"
    },
    {
        "key": "ctrl+f",
        "command": "-editor.action.extensioneditor.showfind",
        "when": "extensionEditorWebviewFocus"
    },
    {
        "key": "alt+t",
        "command": "workbench.action.terminal.focus"
    },
    {
        "key": "ctrl+o",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+shift+n",
        "command": "extension.advancedNewFile"
    },
    {
        "key": "ctrl+alt+n",
        "command": "-extension.advancedNewFile"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+n",
        "command": "-workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+shift+n",
        "command": "newFile.createNewFile"
    },
    {
        "key": "ctrl+alt+n",
        "command": "-newFile.createNewFile"
    },
    {
        "key": "alt+f",
        "command": "gitlens.openWorkingFile"
    },
    {
        "key": "ctrl+shift+z ctrl+shift+p",
        "command": "extension.colorHelper.convert",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+z alt+t",
        "command": "-extension.colorHelper.convert",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+p ctrl+shift+p",
        "command": "extension.colorHelper.pick",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+z alt+p",
        "command": "-extension.colorHelper.pick",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+x ctrl+shift+p",
        "command": "toggleFindCaseSensitive",
        "when": "editorFocus"
    },
    {
        "key": "alt+z alt+q",
        "command": "-toggleFindCaseSensitive",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+p",
        "command": "toggleSearchCaseSensitive",
        "when": "searchInputBoxFocus && searchViewletVisible"
    },
    {
        "key": "alt+z alt+e",
        "command": "-toggleSearchCaseSensitive",
        "when": "searchInputBoxFocus && searchViewletVisible"
    },
    {
        "key": "alt+c",
        "command": "git.openChange"
    },
    {
        "key": "alt+o",
        "command": "editor.action.goToDeclaration",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "f12",
        "command": "-editor.action.goToDeclaration",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "alt+k",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "alt+left",
        "command": "-workbench.action.navigateBack"
    },
    {
        "key": "alt+d",
        "command": "-gitlens.showQuickCommitFileDetails",
        "when": "editorTextFocus && gitlens:enabled && gitlens:keymap == 'alternate'"
    },
    {
        "key": "alt+tc",
        "command": "-toggleFindCaseSensitive",
        "when": "editorFocus"
    },
    {
        "key": "alt+cs",
        "command": "-toggleSearchCaseSensitive",
        "when": "searchInputBoxFocus && searchViewletVisible"
    },
    {
        "key": "alt+c p",
        "command": "-extension.colorHelper.pick",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+tc",
        "command": "-extension.colorHelper.convert",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+c",
        "command": "-gitlens.showQuickCommitFileDetails",
        "when": "editorTextFocus && gitlens:enabled && gitlens:keymap == 'alternate'"
    },
    {
        "key": "alt+;",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "alt+right",
        "command": "-workbench.action.navigateForward"
    },
    {
        "key": "ctrl+shift+f8",
        "command": "workbench.debug.viewlet.action.enableAllBreakpoints"
    },
    {
        "key": "ctrl+f8",
        "command": "workbench.debug.viewlet.action.disableAllBreakpoints"
    },
    {
        "key": "alt+b",
        "command": "-gitlens.toggleFileBlame",
        "when": "editorTextFocus && config.gitlens.keymap == 'alternate' && gitlens:activeFileStatus =~ /blameable/"
    },
    {
        "key": "alt+b",
        "command": "gitlens.toggleLineBlame"
    },
    {
        "key": "ctrl+shift+g b",
        "command": "-gitlens.toggleFileBlame",
        "when": "editorTextFocus && config.gitlens.keymap == 'chorded' && gitlens:activeFileStatus =~ /blameable/"
    },
    {
        "key": "alt+b",
        "command": "gitlens.toggleFileBlame"
    },
    {
        "key": "ctrl+l",
        "command": "-extension.vim_navigateCtrlL",
        "when": "editorTextFocus && vim.active && vim.use<C-l> && !inDebugRepl"
    },
    {
        "key": "ctrl+shift+t",
        "command": "-launcher.terminal"
    },
    {
        "key": "ctrl+shift+t",
        "command": "-extension.tabSpacer"
    },
    {
        "key": "alt+p",
        "command": "workbench.action.reopenClosedEditor"
    },
    {
        "key": "ctrl+shift+t",
        "command": "-workbench.action.reopenClosedEditor"
    },
    {
        "key": "alt+p",
        "command": "-keybindings.editor.toggleSortByPrecedence",
        "when": "inKeybindings"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.debug.panel.action.clearReplAction",
        "when": "inDebugRepl"
    },
    {
        "key": "win+l",
        "command": "-workbench.debug.panel.action.clearReplAction",
        "when": "inDebugRepl"
    },
    {
        "key": "ctrl+shift+y",
        "command": "-workbench.debug.action.toggleRepl"
    },
    {
        "key": "alt+d",
        "command": "workbench.debug.action.focusRepl"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.output.action.clearOutput"
    },
    {
        "key": "ctrl+r ctrl+r",
        "command": "editor.action.rename",
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "f2",
        "command": "-editor.action.rename",
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    }
]
