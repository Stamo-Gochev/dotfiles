if [ -n "$BASH_VERSION" ]; then
  # move to a list of previously visited directories
  # http://linuxgazette.net/109/marinov.html
  alias cd=cd_func
  # smart cd to previous directories, usage "cd 1", better than "cd -2"
  alias cdd=cd_to_func
fi
